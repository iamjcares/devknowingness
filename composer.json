{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"require": {
		"laravel/framework": "5.0.27",
		"intervention/image": "2.1.1",
        "laravel/cashier": "4.0.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"phpspec/phpspec": "~2.1"
	},
	"autoload": {
		"classmap": [
			"database",
			"app/Http/Controllers",
			"app/Models",
			"app/Libraries"
		],
		"psr-4": {
			"HelloVideo\\": "app/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
	},
	"config": {
		"preferred-install": "dist"
	}
}
